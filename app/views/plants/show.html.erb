<!-- Responsive panel example-->

<div class="row">
  <div class="col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title"><%= @plant.display_name %></h3>
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-md-12">
            <dl class="dl-horizontal">

                <img src="data:image/png;base64,"/>
                </dl>

              <dt>Container Size:</dt>
              <dd><%= @plant.category %></dd>


              <dt>Days Between Watering:</dt>
              <dd><%= @plant.dbw %></dd>



            </dl>
            <div class="btn-group btn-group-justified">
              <a href="/plants" class="btn btn-primary">
                <i class="fa fa-chevron-left"></i>
              </a>

              <a href="/plants/<%= @plant.id %>/edit" class="btn btn-warning">
                <i class="fa fa-edit"></i>
              </a>

              <a href="/delete_plant/<%= @plant.id %>" class="btn btn-danger" rel="nofollow">
                <i class="fa fa-trash-o"></i>
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- End responsive panel example -->
